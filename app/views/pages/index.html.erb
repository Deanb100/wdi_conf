<script id="session-box" type="text/x-handlebars-template">
  <span class="session-box draggable" time={{talktime}} venue={{venue}} data-id={{sessionid}}>
    <img src="/images/{{speakerimg}}" alt="" data-id={{sessionid}}>
    <h2 data-id={{sessionid}}>{{speakername}}</h2>
    <p data-id={{sessionid}}>{{talktitle}} @ {{talktime}}:00</p>
  </span>
</script>

<script id="timeslot" type="text/x-handlebars-template">
    <div class="itinerary-row">
      <span class="timeslot" time={{talktime}}>
        <p>{{text}}</p>

      </span>
    </div>
</script>

<script id="overlay" type="text/x-handlebars-template">
  <img src="/images/{{speakerimg}}" alt="">
  <h2>{{speakername}} </h2>
  <p>presents<p></br>
  <h3>"{{talktitle}}"</h3><br>
  <h4>{{talkdesc}}</h4><br>
  <p>{{speakerdesc}}</p><br>

</script>

<script id="attendee-form" type="text/x-handlebars-template">

  <%= form_tag charges_path, id: "payment-form", method: "post" do %>

    <div class="form-row">
        <h4>First Name:</h4>
        <input class="first_name" type="text" size="20" name="first_name" >
    </div>

    <div class="form-row">
        <h4>Last Name:</h4>
        <input class="last_name" type="text" size="20" name="last_name" >
    </div>

    <div class="form-row">
        <h4>Phone Number :</h4>
        <input class="email" type="text" size="20" name="email" >
    </div>

    <div class="form-row">
        <h4>Email:</h4>
        <input class="email" type="text" size="20" name="email" >
    </div>

    <div class="form-row">
        <h4>Card Number</h4>
        <input type="text" size="20" data-stripe="number">
    </div>

    <div class="form-row">
        <h4>Expiration (MM/YY)</h4>
        <input type="text" size="2" data-stripe="exp_month">
      <h4> / </h4>
      <input type="text" size="2" data-stripe="exp_year">
    </div>

    <div class="form-row">
        <span>CVC</span>
        <input type="text" size="4" data-stripe="cvc">
    </div>
    <span class="payment-errors"></span>

    <input id="sessionsAdded" type="hidden" name="name" value="">
    <input type="submit" class="submit" value="Submit Payment">
  <% end %>


</script>
